{
  "name": "cabris-explorateurs",
  "version": "1.0.0",
  "private": true,
  "description": "my blog",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "bundle update && yarn upgrade",
    "prebuild": "run-s clean:dist",
    "build": "run-s webpack images jekyll",
    "prestart": "run-s clean:dist",
    "start": "run-p webpack:dev images:dev jekyll:dev",
    "clean": "run-p clean:dist clean:cache",
    "clean:dist": "shx rm -rf dist/",
    "clean:cache": "shx rm -rf .img-cache/",
    "webpack": "webpack -p",
    "webpack:dev": "webpack -d -w",
    "images": "gulp build",
    "images:dev": "gulp watch",
    "jekyll": "bundle exec jekyll build",
    "jekyll:dev": "bundle exec jekyll serve --config _config.yml,_config_dev.yml"
  },
  "repository": {
    "type": "git",
